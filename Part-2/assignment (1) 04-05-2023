#  Write a program to print given year is leap year or not using Class

class Years:
    def Leap_year(self,year_num):
        if year_num%4==0 and year_num%100!=0 or year_num%400==0:
            return True
        else:
            return False


year_num=int(input("Enter Which year you want to check Leap year or not :"))

o=Years()# Creating Object to the Years Class

if o.Leap_year(year_num):
    print("Given Year (%d) is leap Year"%(year_num))
else:
    print("Given Year (%d) is Not a leap Year"%(year_num))
