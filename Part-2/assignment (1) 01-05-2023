Player_Details={}
n=int(input("Enter Number of Players:"))
for i in range(1,n+1):
    pno=int(input("\nEnter %d Player Number:"%(i)))
    pname=input("Enter Player Name:")
    Player_Details[pno]=pname

print("\n\tPlayers Details")
for i in Player_Details:
    print("\t",i," => ",Player_Details[i])
